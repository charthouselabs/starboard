{
  "name": "starboard-indexer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "generate": "squid-gen-abi",
    "typegen": "squid-evm-typegen ./src/abi ./abi/mock-pricefeed-abi.json",
    "start": "docker compose up -d; tsc; node -r dotenv/config lib/main.js ",
    "codegen": "squid-typeorm-codegen",
    "migrate:generate": "squid-typeorm-migration generate",
    "migrate:create": "squid-typeorm-migration create",
    "migrate:apply": "squid-typeorm-migration apply",
    "serve": "squid-graphql-server"
  },
  "dependencies": {
    "@subsquid/archive-registry": "^3.3.2",
    "@subsquid/batch-processor": "^0.0.0",
    "@subsquid/evm-abi": "^0.3.1",
    "@subsquid/evm-processor": "^1.27.3",
    "@subsquid/fuel-objects": "^0.0.4",
    "@subsquid/fuel-stream": "^1.0.4",
    "@subsquid/graphql-server": "^4.11.0",
    "@subsquid/typeorm-migration": "^1.3.0",
    "@subsquid/typeorm-store": "^1.5.1",
    "@subsquid/util-internal-json": "^1.2.3",
    "dotenv": "^16.6.1",
    "pg": "^8.16.3",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@subsquid/cli": "^3.2.3",
    "@subsquid/evm-typegen": "^4.4.0",
    "@subsquid/squid-gen-evm": "^2.0.0",
    "@subsquid/typeorm-codegen": "^2.0.2",
    "@types/node": "^22.17.1",
    "tsx": "^4.20.4",
    "typescript": "5.9.2"
  }
}
